## The function is currently defined as
function (residuals, error) 
{
    val <- residuals$G
    if (error == "lnorm") {
        val <- log(val)
    }
    mean_val <- mean(val)
    rss <- sum((val - mean_val)^2)
    return(rss)
  }
}
